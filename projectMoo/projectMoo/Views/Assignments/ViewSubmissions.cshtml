@model projectMoo.Models.ViewModels.SubmissionViewModel

<div class="coursetable">
    <table class="table table-hover courses">
        <thead>
            <tr>
                <th>Milestone Title</th>
                <th>State</th>
                <th>File</th>
                <th>Date</th>
            </tr>
        </thead>
        @foreach (var submission in Model.Submissions)
        {
            <tr class="cursor-change">
                <td class="lead">@Model.Milestone.Title</td>
                <td>@submission.State</td>
                <td><a href="http://localhost:1773/Assignments/Download?file=@submission.FileID" target="_blank">Download File</a></td>
                <td><span class="badge">@submission.Date.ToShortDateString()</span></td>
            </tr>


        }
    </table>
</div>